{
  "schemaVersion": 1,
  "id": "fehemi",
  "version": "${version}",

  "name": "Fabricae Ex Nihilo Emi Plugin",
  "description": "Emi Plugin for FEH",
  "authors": [
    "BulbyVR"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "GPL-3.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "emi": [
      {
        "adapter": "scala", 
        "value": "net.bulbyvr.fehemi.FehEmiPlugin"
      }
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "fabric-language-scala": "*",
    "emi": "*",
    "fabricaeexnihilo": "*"

  }
}
